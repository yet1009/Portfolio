<!DOCTYPE html>
<html lang = "ko" >
 <head>
  <title>Manchester United</title>
  <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0"/>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  <style>
		*{margin:0; padding:0;}
		li{list-style-type:none;}
		a:link,a:visited{text-decoration:none; color:#333;}
		a:hover,a:focus{text-decoration:underline;}
		img{border:none;}

		/*############## 전체 해상도 ##############*/
			#wrap{min-width:1025px; max-width:1920px; width:100%; margin:0 auto;}
			#headerWrap{width:100%; height:100px;  background-color:#c70101;}
		
		/*############ 실제 사용하는 header 영역 ################*/
			#header{width:1600px; height:100px; margin:0 auto;  position:relative;}
		
		/*### logo ###*/
			#logo{width:80px; height:80px; position:absolute; left:50%; top:0; padding:10px 0;}
			#logo a{display:block; width:100%; height:100%;}
			#logo img{width:100%;}
		
		/*######## gnb(gnb 전체 사용영역) #######*/
			#gnb{width:800px; height:100%;}

			.hamburger{width:100px; height:100%; float:left;}
			.hamburger p{width:70px; height:70px; margin:15px auto; }
			.hamburger p img{width:100%;}

			.nav{width:700px; height:100%; line-height:100px;  float:left;}
			.nav li{display:inline; padding:0 40px;}
			.nav li a span{font-weight:bold; color:#fff; font-size:16px;}
			
			.iconbox{width:260px; height:100%; background-color:#c70101; position:absolute; left:1140px; top:0;}	
			.iconbox p{width:50%; height:100%; float:left;}
			.iconbox p a{display:block; width:100%; height:100%;  text-align:center; line-height:100px; color:#fff;}
			.iconbox p img{width:25%; vertical-align:middle;}

			.searchBox{width:100%; height:70px; background-color:rgba(255,255,255,1); position:absolute;  /*top:0px; z-index:9;*/ border-radius:0 0 10px 10px; /*display:none;*/}
			.searchBox p{width:1100px; height:100%; text-align:right; position:relative; left:400px; top:0;}
			.searchBox label{font-size:30px; line-height:65px; background-image:-webkit-linear-gradient(right, #cc0, #e00);  -webkit-background-clip:text; color:transparent;}
			.searchBox input[type="text"]{width:600px; height:40px; border:none; border-bottom:3px solid #ccc; background-color:rgba(255,255,255,0.4); vertical-align:middle; text-indent:10px; font-size:17px; outline:none;}
			.searchBox input[type="button"]{width:100px; height:40px; border:none; border-radius:10px;  background-color:#ccc;}
			.searchBox input[type="button"]:hover{background-image:linear-gradient(to top, #c70101, gold); 
					background-image:-webkit-linear-gradient(to top,#c70101, gold);}
			.searchBox img{width:30px;}
			
			.spons_logo{width:200px; height:100%;  background-color:#000; /*background-color:#c70101;*/ position:absolute; right:0; top:0;}
			.spons_logo p{width:50%; height:50px; text-align:center; line-height:60px; float:left;}
			.spons_logo p img{height:50%; cursor:pointer;}
				
			.spons_logo p:first-child{line-height:65px;}
			.spons_logo p:first-child img{height:65%;}
		
		/*################## depthWrap #####################*/

			#depthWrap{width:100%; height:400px; position:absolute; background-image:linear-gradient(to top,rgb(154 149 149 / 62%), rgb(35 32 30 / 57%)); 
					background-image:-webkit-linear-gradient(to top,rgb(154 149 149 / 62%), rgb(35 32 30 / 57%)); left:0; top:100px; z-index:10; display:none;}
			#contentWrap{width:1600px; height:100%; /*background-color:gold;*/ margin:0 auto; position:relative; }
			#allcont{width:1500px; padding-left:100px; }
		
		
			.contList{width:100%; height:400px;  background-image:url("img/contbg1.jpeg"); background-position:right; background-size:50%; background-repeat:no-repeat;} 
			.contList li{width:140px; height:60px; text-align:center; line-height:60px; float:left;}
			.contList li ul li a{color:#fff; font-size:16px; font-weight:500;}
 
			/*.contList{ background-image:url("img/contbg.jpg"); background-position:center; background-size:100%;}*/
			.contList li ul:hover li{/*background-color:rgba(0,0,0,0.7);*/ background-color:rgba(199,1,1,0.5);}
			
			.contList a:hover,.contList a:focus{/*background-image:-webkit-linear-gradient(right, #cc0, #e00);  -webkit-background-clip:text; color:transparent;*/ color:#000;}


			/*############################## container #########################*/

				#container{width:100%;}
			

			/*############ section0 ############*/
				#section0{width:100%; position:relative;}
				#subvisual{width:100%; height:800px; position:relative; background-image:url("img/subvisual3.jpg"); background-position:bottom; background-size:100%; background-repeat:no-repeat;}
				#subvisual .opabackground{width:100%; height:800px; background-color:rgba(0,0,0,0.3); position:absolute; left:0; top:0;}
			
				#info{width:900px; height:600px; position:absolute; left:50%; top:50%; transform:translate(-50%,-50%); /*background-color:gold;*/}

				#info .cen{width:100%; height:200px; /*background-color:gold; position:absolute; left:50%; margin-left:-100px; top:40%;*/ z-index:3; /*background-color:pink;*/}	
				
				#info .schedule{width:100%; height:90px; color:#fff;  font-weight:800; text-align:center; font-size:17px;line-height:30px;}
			
				#info .lefttime{width:280px; height:50px; padding:10px; margin:0 auto; border:1px solid #ddd;}
				#info .lefttime .tagh{width:60px; padding-right:10px; float:left;}
				#info .lefttime .tagh img{width:100%;}
				#info .lefttime .time{width:210px; height:52px; background-color:rgba(75,76,82,0.5); color:#fff;text-align:center; font-size:22px;  line-height:52px; font-weight:600; float:left;}
				
				

				.team{width:100%; height:370px; padding-top:30px;}
				.team li{width:300px; height:100%; text-align:center; float:left;}
				.team li img{width:55%;}
				.team .vs{font-size:35px;color:#fff; line-height:200px;}
				.team .clubt{font-size:30px; letter-spacing:2px; color:#fff; font-weight:700;}

			/*############### section1 ######################*/
			
				#section1{width:100%; height:500px; /*border:1px solid red;*/overflow:hidden; /*background-color:rgba(0,0,0,1);*/ padding:10px 0 50px;}

				#news{width:3670px; height:100%; /*background-color:gold; transition:all .5s linear; */ }
				#news li{width:350px; height:100%; margin-right:15px; overflow:hidden; border-radius:20px; border:1px solid #ccc; float:left; }
				#news .newsimgs{width:100%; overflow:hidden;}	

				#news li:hover img{transform:scale(1.1);}

				#news .newsimgs a{display:block; width:100%; height:100%;}
				#news .newsimgs img{width:100%; transform:scale(1); transition:all .5s;}
				#news .newsdesc{width:310px; height:140px; padding:10px 20px 0 20px; background-color:#fff;}
				#news .newsdesc h3{width:100%; height:55px; font-size:20px; padding-bottom:20px;}
				#news li:hover h3 a{text-decoration:none; background-image:-webkit-linear-gradient(right, #cc0, #e00);  -webkit-background-clip:text; color:transparent;}
				#news .newsdesc .box{width:150px; height:50px; margin:0 auto; background-color:#c70101; text-align:center; line-height:50px; font-weight:700; font-size:18px;}
				#news .newsdesc .box a{color:#fff;}
				#news li:hover .box{background-color:rgba(167,9,9,1);}
				#news li:hover .box a{color:#fff; text-decoration:none;}
			
				
				#news .bignews{width:700px; height:100%;}
				#news .bignews .newsimgs{width:100%; overflow:hidden;}
				#news .bignews .newsimgs img{width:100%; transform:scale(1);}
				#news .bignews:hover img{transform:scale(1.1);}
				#news .bignews .newsdesc{width:660px; height:140px; padding:10px 20px 0 20px; background-color:#fff;}
				#news .bignews .newsdesc h3{width:100%; height:50px; font-size:20px; padding:0; text-align:center; font-size:24px;}
				#news .bignews .newsdesc span{display:block; width:100%; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; padding-bottom:10px;}
				
				#btn1{width:100%; height:50px; /*background-color:pink;*/ position:relative;}
				#btn1 span{position:absolute;}
				#btn1 span img{width:30px; cursor:pointer;}
				#btn1 .prev1{right:55px; top:10px;}
				#btn1 .next1{right:10px; top:10px;}
				

				#section1:after{content:""; display:block; clear:both;}

			/*############ section2 #################*/

				#section2{width:100%;}
				#section2 .game{width:1440px; height:400px; padding-top:50px;margin:0 auto; }


				.pregame{width:620px; height:340px; padding:10px 50px 0 50px; /*background-image:linear-gradient(bottom, red, yellow);background-image:-webkit-linear-gradient(bottom,red, yellow);*/background-image:-webkit-linear-gradient(bottom,hsl(0deg 61% 63%), hsl(33deg 68% 72%)); float:left;}
 
				.pregame h4{width:100%; height:30px; margin-bottom:10px; font-size:24px; font-weight:800; border-left:10px solid red; text-indent:10px; color:#fff;}
				.pregame ul{width:100%; height:300px; }
				.pregame li{width:310px; height:100%; float:left;}
				.pregame .date{width:35px; height:100%; font-size:20px; text-align:center; color:#fff; background-color:#000; float:left;}
				.pregame .result{width:250px; padding-left:25px;height:100%;float:left;}
				.pregame .result .league{width:100%; font-size:15px; font-weight:900;  padding-bottom:30px; color:#3d0842;}
				.pregame .result .home{width:100%; font-size:26px; padding-bottom:20px; color:red; font-weight:600;}
				.pregame .result .away{width:100%; font-size:26px; padding-bottom:50px; color:#fff;}
				/*.pregame .result span{color:#333;}*/

				.pregame .matchroom{width:120px; height:40px; background-color:#c70101; text-align:center; line-height:40px; font-size:19px; font-weight:400;}
				.pregame .matchroom a{color:#fff;}

				.pregame .matchroom:hover{background-image:linear-gradient(to top, red, gold); background-image:-webkit-linear-gradient(to top,red, gold); background-image:-o-linear-gradient(to top,red, gold); background-image:-moz-linear-gradient(to top,red, gold); background-image:-ms-linear-gradient(to top,red, gold);}
				.pregame .matchroom:hover a{text-decoration:none;}
			
				.nextgame{width:720px; height:350px; float:left;}
				/*.nextgame h4{width:100%; height:50px; font-size: 24px; text-indent:25px; font-weight:800px;}
				.nextgame .highlight{width:100%; height:300px; }
				.highlight video{width:100%; }*/
				.nextgame p{width:100%; height:100%; background-color:#333;}
				.nextgame p iframe{width:100%; height:100%; border:none;}


			
			/*############## visual ######################*/
				#visual{width:100%; height:750px; overflow:hidden; position:relative; margin-bottom:50px; }
				#visual ul{width:400%; height:100%; margin-left:-100%; }
				#visual ul li{width:25%; height:100%; float:left;}
				#visual ul li img{width:100%;}
				

				.btn p{width:50px; height:100px; position:absolute; top:50%; transform:translatey(-50%);}
				.btn p img{width:100%; height:100%;}

				#prevBtn{left:5%; cursor:pointer;}
				#nextBtn{right:5%; cursor:pointer;}
				

				#moveL{width:450px; height:20px; /*background-color:rgba(255, 255,0,0.5);*/ position:absolute; left:-450px; top:100px; animation:yourani 1 1s 1.5s; background-image:url("img/manulogo.png"); background-size:0%; background-position:center; background-repeat:no-repeat;}
				#moveT{width:550px; height:20px; /*background-color:rgba(0,0,0,1);*/ position:absolute; left:-900px; top:100px; text-align:center;  color:#fff; line-height:250px; font-size:0; animation:myani1 1 1s 2s; font-weight:bold;}
				#moveT span{background-image:-webkit-linear-gradient(right, #cc0, #e00);  -webkit-background-clip:text; color:transparent;}
				
				@keyframes yourani{
					0%{transform:rotatex(0deg);}
					100%{transform:rotatex(360deg);}
				}
				
				@keyframes myani1{
					0%{transform:rotatex(0deg);}
					100%{transform:rotatex(360deg);}
				}
			

			/*############ section3 ###########*/
				#section3{width:100%; height:700px; padding-top:50px; /*background-image:-linear-gradient()#e11922;*/ /*background-color:#efefef;*/ background-color:#000;}
				#section3 h2{width:1600px; font-size:30px; color:#fff; padding-bottom:25px; margin:0 auto;}
				#youtube{width:1600px; height:600px; overflow:hidden; margin:0 auto;}
				#youtube ul{width:100%; height:100%; margin:0 auto;}
				#youtube li{width:318px; height:598px; border:1px solid #333; float:left; position:relative;}	
				
				#youtube .thumb{width:100%; height:100%; overflow:hidden;}
				#youtube .thumb img{width:100%; transform:scale(1); transition:transform 0.7s linear;}
				#youtube .thumb:hover img{transform:scale(1.05);}

				#youtube .youtu_info{width:298px; height:195px; background-color:rgba(0,0,0,1); padding:10px 10px 0 10px; position:absolute; left:0; bottom:0px;}
				#youtube .youtu_info .but{width:45px; height:45px;}
				#youtube .youtu_info .but img{width:100%;}
				#youtube .youtu_info .youtu_title{width:100%; height:50px; color:#fff; font-size:19px; padding:15px 0 40px;}
				#youtube .youtu_info .youtu_title span{color:#fff;}
				#youtube .youtu_info .youtu_date span{color:#ccc;}
				#youtube .youtu_info .youtu_date i{color:#ccc; font-style:normal;}

			/*############## section4 #################*/

				#section4{width:100%; padding:50px 0; background-color:#fff;}
				#uniform{width:100%; height:470px;}
				#uniform ul{width:100%;}
				#uniform li{width:24%; border:1px solid #ccc; background-color:#fff; float:left;}
				#uniform li img{width:100%; opacity:0.6;}
				#uniform li img:hover{opacity:1;}

				#uniform ul .uniform_title{border:none;}		
				#uniform ul .uniform_title .uni_head{width:450px; padding-right:23px; display:block; font-size:30px; text-align:right; font-weight:800; color:#000;}
				#uniform .uniform_title .shopbox{width:150px; height:65px; background-color:#db1d24; font-size:22px; text-align:center; line-height:65px; position:relative; left:-300px; top:150px; transition:all 1.2s linear;}
				#uniform .uniform_title .shopbox a{color:#fff; }

				#uniform ul:hover .shopbox{left:300px; top:150px;}

			/*################### section5 ########################*/

				#section5{width:100%; padding:50px 0; background-color:rgba(0,0,0,0.9);}
				#players{width:1640px; height:550px; margin:0 auto; }
				#players h2{width:100%; height:50px; text-align:center; color:#fff; font-size:30px;}

				#players ul{width:100%; height:500px;}
				#players ul li{width:400px; height:450px; padding:25px 0; margin-right:10px; position:relative; cursor:pointer; float:left;}
				#players ul li .playerimg{width:100%; height:100%}
				#players li .playerimg img{width:100%;}
				
				/*이미지 크기: 400px * 450px*/
				
				#players .playerInfo{width:100%; height:200px; position:absolute; left:0; bottom:25px; /*background-image:linear-gradient(0deg,rgba(8,14,31,.9) 26%, rgba(15,17,40,.7) 50%, rgba(24,23,51,0) 100%);*/
				background-image:linear-gradient(0deg,rgba(123,34,8,.7) 25%, rgba(121,34,29,.4) 50%, rgba(0,0,0,0) 100%); text-align:center; overflow:hidden;  white-space:nowrap; transition:all .5s linear;}
				#players .playerInfo .player_name{font-size:8em; font-weight:600; text-align:center; background-image:linear-gradient(180deg,hsla(0,0%,93%,.5),hsla(0,0%,100%,0) 90%); /*background-image:-webkit-linear-gradient(bottom, #777, #333);*/  -webkit-background-clip:text; color:transparent; position:absolute; left:0; font-weight:600; letter-spacing:-2px;} 

				#players li:first-child .player_name{font-size:7.5em;}
				#players li:last-child .player_name{left:5%;}
				
				.playerInfo .play_number,.playerInfo .play_lastname,.playerInfo .play_firstname{line-height:6em; font-size:3em; font-weight:700; margin-left:3px; color:#fff; background-image: linear-gradient(180deg,hsla(0,0%,100%,.5),rgba(0,0,0,.2)),linear-gradient(#fff,#fff);
				-webkit-background-clip:text; color:transparent;}

				.playerInfo .play_lastname{font-weight:400;}

				.playerInfo .play_position{font-size:2em;}
				

				#players .playerStats{width:100%; height:100px; text-align:center; color:#fff; position:relative; left:0; top:0; opacity:0; transition:all .5s linear;}
				#players .playerStats li{width:33%; height:100%; margin:0; padding-top:10px; /*border-top:1px dotted #ccc;*/ display:inline-block; font-weight:600;}
				#players .playerStats li p i{/*color:#c70421;*/ color:orange;}


				#players ul li:hover .playerInfo{bottom:100px; opacity:0;}
				#players ul li:hover .playerStats{top:-60px; opacity:1;}

			/*#################### section6 #####################*/
				#section6{width:100%; height:600px;  margin:25px auto; background-image:url("img/trophybg.jpg"); background-position:center; background-repeat:no-repeat; background-size:100%; padding:50px 0;}
				#trophy{width:1440px; height:100%; /*background-color:rgba(0,0,0,0.5);*/ overflow:hidden; margin:0 auto;}

				#trophy h2{width:180px; height:500px; padding-top:100px;  padding-left:40px; color:#fff; font-size:35px; float:left;}
				#trophy .move{width:1200px; height:500px; margin-top:50px; margin-right:40px; overflow:hidden; }
				#trophy ul{width:4500px; height:100%; background-color:rgba(0,0,0,1); opacity:1; float:left; animation:myani 14s infinite linear;}
				#trophy ul li{width:300px;  height:400px; padding:50px 0; float:left;}
				
				#trophy li p{width:100%;}
				#trophy li p img{width:100%;}


				#trophy li .txtbox{width:300px; height:400px; background-color:#000; opacity:1; position:relative; left:0; top:-400px; text-align:center; transition:opacity 0.6s linear;}
				#trophy ul li:hover .txtbox{opacity:0.3;}

				#trophy li div span{display:block; width:100%; height:100%; color:#fff; font-size:30px; text-transform:uppercase;}
				#trophy li div .bot{line-height:700px;}
				
				@keyframes myani{
					0%{transform:translatex(0);}
					100%{transform:translatex(-3300px);}
				}


				#section6:after{content:""; display:block; clear:both;}

			/*################## footer ###################################*/
				#footerWrap{width:100%;}
				#footer{width:100%;}
				#sponsor{width:100%; height:130px; margin:50px auto 0; background-color:#eee; }
				#sponsor ul{width:1500px; margin:0 auto;}
				#sponsor li{width:80px; height:100%; line-height:120px; margin:0 10px; display:inline-block;}
				#sponsor li a img{width:100%; vertical-align:middle;}
				
				#footer .footertxt{width:100%; height:50px; background-color:#333;}
				#footer .footertxt ul{width:1440px; height:100%; line-height:50px; margin:0 auto;}
				#footer .footertxt ul li{display:inline; padding:0 15px;}
				.footertxt ul li a{color:#efefef;}

				.linkc{width:100%; background-color:rgba(0,0,0, 0.6);}
				.linkc ul{width:190px; height:60px; padding-top:20px; margin:0 auto; }
				.linkc li{width:40px; height:40px; color:#333; border-radius:20px; margin-right:5px;background-color:#aaa; float:left;}
				.linkc li a{display:block; width:100%;}
		

				.linkc .signup{width:90px; height:40px; text-align:center; line-height:40px; background-color:#c70101;}
				.linkc .signup:hover{background-color:rgba(199,1,1,0.8);}
				.linkc .signup:hover a{text-decoration:none;}
				.linkc .signup span{color:#fff;}
				
				.linkc .login:last-child{margin-right:0;}
				.linkc .login{width:90px; height:40px; text-align:center; line-height:40px;}
				.linkc .login:hover a{color:#fff; text-decoration:none;}

				.copyright{width:100%; height:50px; background-color:#000;}
				.copyright p{width:100%;  text-align:center; line-height:50px; margin:0 auto; color:#aaa; font-size:14px;}

		
				#footer:after{content:""; display:block; clear:both;}

				@media all and (min-width:1915px) and (max-width:1920px){
			
					/*body{background-color:gold;}*/
				
				}
